<?xml version="1.0" encoding="utf-8"?>
<table name="users" prefix="sys" timestamps="yes">
	<field name="name" type="string" size="short"/>
	<field name="user" type="string" size="short" length="64"/>
	<field name="pass" type="string" size="short" length="64"/>
	<field name="salt" type="string" size="short" length="64"/>
	<xref name="group" alias="user" table="groups">
		<field name="default" type="bool"/>
	</xref>
</table>
